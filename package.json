{
  "name": "X-Effect",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bill Gryta",
  "license": "MIT",
  "scripts": {
    "start": "concurrently -k \"yarn run flask-server\" \"yarn run webpack-server\"",
    "build": "webpack --config webpack.prod.js",
    "webpack-server": "webpack-dev-server --config webpack.dev.js",
    "clean-install": "rm -rf node_modules/ yarn.lock && yarn cache clean && yarn install",
    "lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore; true",
    "format": "cross-env prettier-eslint --write  \"src/**/*.js\"",
    "flask-server": "cross-env FLASK_ENV=development FLASK_APP=xeffect/app.py flask run",
    "deploy": "yarn build && zappa update prod",
    "get-config": "aws s3 cp s3://xeffect-config/config.py ./xeffect/",
    "save-config": "aws s3 cp ./xeffect/config.py s3://xeffect-config/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "axios": "^0.18.0",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-facebook-login": "^4.0.1",
    "react-google-login": "^3.2.1",
    "react-modal": "^3.4.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "validator": "^10.2.0",
    "webpack": "^4.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.9.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
