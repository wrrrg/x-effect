{
  "name": "X-Effect",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bill Gryta",
  "license": "MIT",
  "scripts": {
    "start": "yarn run webpack-server & yarn run flask-server",
    "build": "webpack --config webpack.prod.js",
    "webpack-server": "webpack-dev-server --config webpack.dev.js",
    "flask-server": "FLASK_ENV=development FLASK_APP=app.py flask run",
    "deploy": "yarn run build && zappa update prod",
    "clean-install": "rm -rf node_modules/ yarn.lock && yarn cache clean && yarn install",
    "lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore; true"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-modal": "^3.4.4",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "validator": "^10.2.0",
    "webpack": "^4.10.2"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.9.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
